-- Only run in the specified game
if game.PlaceId ~= 106337932664716 then
    return
end

-- Load Fluent and addons
local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()

-- Create window
local Window = Fluent:CreateWindow({
    Title = "1+ Jump per Step" .. Fluent.Version,
    SubTitle = "by iamnotldsteam",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = true,
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.LeftControl
})

-- Create tabs
local Tabs = {
    Main = Window:AddTab({ Title = "OP!!!!", Icon = "" }),
    Issue = Window:AddTab({ Title = "request", Icon = ""}),
    Settings = Window:AddTab({ Title = "Settings", Icon = "" })
}


SaveManager:SetLibrary(Fluent)
InterfaceManager:SetLibrary(Fluent)

SaveManager:IgnoreThemeSettings()
SaveManager:SetIgnoreIndexes({})
InterfaceManager:SetFolder("FluentScriptHub")
SaveManager:SetFolder("FluentScriptHub/specific-game")
InterfaceManager:BuildInterfaceSection(Tabs.Settings)
SaveManager:BuildConfigSection(Tabs.Settings)

Window:SelectTab(1)

Fluent:Notify({
    Title = "Fluent",
    Content = "The script has been loaded successfully! ✅",
    Duration = 8
})








Tabs.Issue:AddButton({
    Title = "Discord",
    Description = "Copy to Clipboard Discord Link",
    Callback = function()
        -- Copy the Discord invite link to the player's clipboard
        if setclipboard then
            setclipboard("https://discord.gg/mR2gVagPzG")
            print("Discord invite copied to clipboard!")
        else
            print("Your executor does not support setclipboard.")
        end
    end
})

-- ⚠️ Merchant Error (Static)
--------------------------------------------------
    Tabs.Issue:AddParagraph({
        Title = "request",
        Content = "Join Discord to request"
    })













































local Toggle = Tabs.Main:AddToggle("InfiniteWinToggle", {Title = "Infinite win op!!", Default = false})
local running = false
local player = game.Players.LocalPlayer

-- The target CFrame
local targetCFrame = CFrame.new(
    -488.076843, 5446.19873, -33.1538239, 
    0, 0, -1, 
    0, 1, 0, 
    1, 0, 0
)

-- Loop function to teleport
local function loopTeleport()
    spawn(function()
        while running do
            local character = player.Character
            if character and character:FindFirstChild("HumanoidRootPart") then
                character.HumanoidRootPart.CFrame = targetCFrame
            end
            task.wait(0.1) -- teleport every 0.1 second
        end
    end)
end

-- Toggle callback
Toggle:OnChanged(function()
    running = Toggle.Value
    print("Teleport Toggle changed:", running)
    if running then
        loopTeleport()
    end
end)
















local Toggle = Tabs.Main:AddToggle("MyToggle", {
    Title = "Rebirth",
    Default = false
})

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local hrp = character:WaitForChild("HumanoidRootPart")

-- Wait for the level label
local lvlLabel = player:WaitForChild("PlayerGui")
                      :WaitForChild("HUD")
                      :WaitForChild("RebirthFrame")
                      :WaitForChild("Bar")
                      :WaitForChild("LvlLabel")

-- Get the Rebirth remote
local rebirthRemote = ReplicatedStorage:WaitForChild("Shared")
                                  :WaitForChild("Networking")
                                  :WaitForChild("Remotes")
                                  :WaitForChild("Rebirth")

-- Function to extract two numbers from text like "30/31"
local function getTwoNumbers(text)
    local num1, num2 = text:match("(%d+)%s*/%s*(%d+)")
    if num1 and num2 then
        return tonumber(num1), tonumber(num2)
    end
end

-- Loop flag
local running = false

-- Target CFrame
local targetCFrame = CFrame.new(128.5, 73.875, -31, 0, 0, 1, 0, 1, 0, -1, 0, 0)

-- Function to run the loop
local function rebirthLoop()
    local initialTeleportDone = false
    while running do
        -- Teleport once at the start of the toggle
        if not initialTeleportDone and character and hrp then
            hrp.CFrame = targetCFrame
            initialTeleportDone = true
        end

        local num1, num2 = getTwoNumbers(lvlLabel.Text)
        if num1 and num2 then
            if num2 - num1 == 1 then
                rebirthRemote:FireServer()
                wait(1) -- wait 1 second after rebirth
                -- Teleport again after rebirth
                if character and hrp then
                    hrp.CFrame = targetCFrame
                end
            end
        end
        wait()
    end
end

-- Toggle callback
Toggle:OnChanged(function(value)
    running = value
    if running then
        spawn(rebirthLoop) -- start the loop in a new thread
    end
end)














local Toggle = Tabs.Main:AddToggle("BuyMaxStepToggle", {Title = "Buy Max Step", Default = false})
local running = false
local player = game:GetService("Players").LocalPlayer
local statsFolder = player:WaitForChild("StatsFolder")
local wins = statsFolder:WaitForChild("Wins")

-- Function to fire the button and delete it
local function fireButton()
    local button = workspace.Map.Buttons:GetChildren()[4]:FindFirstChild("Main")
    if button then
        -- Fire TouchInterest
        for _, obj in ipairs(button:GetDescendants()) do
            if obj:IsA("TouchTransmitter") then
                firetouchinterest(player.Character.HumanoidRootPart, button, 0)
                firetouchinterest(player.Character.HumanoidRootPart, button, 1)
            end
        end
        -- Delete the button
        button:Destroy()
    end
end

-- Loop function to check wins
local function checkWinsLoop()
    spawn(function()
        while running do
            if wins.Value >= 35000 then
                fireButton()
                running = false
                -- Programmatically turn off the toggle depending on your UI library
                if Toggle.Set then
                    Toggle:Set(false) -- some libraries
                elseif Toggle.Update then
                    Toggle:Update(false) -- alternative method
                elseif Toggle.Value ~= nil then
                    Toggle.Value = false -- fallback
                end
                break
            end
            task.wait(0.5) -- check every 0.5 second
        end
    end)
end

-- Toggle callback
Toggle:OnChanged(function()
    running = Toggle.Value
    print("Toggle changed:", running)
    if running then
        checkWinsLoop()
    end
end)





